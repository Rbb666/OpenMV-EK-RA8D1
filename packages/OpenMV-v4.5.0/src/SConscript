import os
from building import *

src = []
objs = []
cwd  = GetCurrentDir()

path    = [cwd + '/lib/libtf']

objs = DefineGroup('Openmv', src, depend = [''], CPPPATH = path)

list = os.listdir(cwd)
for item in list:
    if os.path.isfile(os.path.join(cwd, item, 'SConscript')):
        objs = objs + SConscript(os.path.join(item, 'SConscript'))

Return('objs')
